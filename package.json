{
  "name": "f-odata",
  "version": "0.0.1",
  "description": "FuryTech.Odata server side implementation for Node.JS",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run dev-test",
    "coverage": "nyc report --reporter=lcov",
    "dev-test": "tsc && tsc -p tsconfig.test.json && nyc mocha -p tsconfig.test.json test-js/test/index.js",
    "dev-test-watch": "mocha-typescript-watch -p tsconfig.test.json test-js/test/index.js",
    "mock": "tsc && node dist/mock.js",
    "cxsd": "cxsd http://docs.oasis-open.org/odata/odata/v4.0/errata03/os/complete/schemas/edm.xsd"
  },
  "nyc": {},
  "keywords": [
    "OData",
    "ExpressJS",
    "TypeScript",
    "REST"
  ],
  "author": "Gallay Lajos",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/chai-http": "0.0.30",
    "@types/express": "^4.0.35",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cxsd": "^0.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.22",
    "nyc": "^10.1.2",
    "typescript": "^2.2.1"
  }
}
